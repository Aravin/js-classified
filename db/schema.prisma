generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model location {
  id           Int     @id @default(autoincrement())
  name         String?
  dist         String? // District
  state        String?
  state_code   String?
  country      String?
  country_code String?
  pincode      String?
  loc_type     String?
  coords       String?
  bbox         String?
}